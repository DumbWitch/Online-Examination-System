<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegistrationForm</servlet-name>
        <servlet-class>demotest.RegistrationForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginForm</servlet-name>
        <servlet-class>demotest.LoginForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>demotest.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>demotest.Dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminForm</servlet-name>
        <servlet-class>demotest.AdminForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminProfile</servlet-name>
        <servlet-class>demotest.AdminProfile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>demotest.DeleteAccount</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCourse</servlet-name>
        <servlet-class>demotest.DeleteCourse</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCourses</servlet-name>
        <servlet-class>demotest.AddCourses</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddQuestions</servlet-name>
        <servlet-class>demotest.AddQuestions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteQuestion</servlet-name>
        <servlet-class>demotest.DeleteQuestion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditAdmin</servlet-name>
        <servlet-class>demotest.EditAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegistrationForm</servlet-name>
        <url-pattern>/RegistrationForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginForm</servlet-name>
        <url-pattern>/LoginForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminForm</servlet-name>
        <url-pattern>/AdminForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminProfile</servlet-name>
        <url-pattern>/AdminProfile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/DeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCourse</servlet-name>
        <url-pattern>/DeleteCourse</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCourses</servlet-name>
        <url-pattern>/AddCourses</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddQuestions</servlet-name>
        <url-pattern>/AddQuestions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteQuestion</servlet-name>
        <url-pattern>/DeleteQuestion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditAdmin</servlet-name>
        <url-pattern>/EditAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
